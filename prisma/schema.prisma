generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model SaleOffer {
  id          Int     @id @default(autoincrement())
  description String?
  priceInPLN  Float
  brand       String
  model       String
  mileageInKm Int
  imageLink   String?
  location    String
}
